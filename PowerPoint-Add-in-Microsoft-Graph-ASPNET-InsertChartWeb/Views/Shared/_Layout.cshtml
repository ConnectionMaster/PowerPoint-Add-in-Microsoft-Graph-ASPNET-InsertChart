<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    <script type="text/javascript" src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-2.2.1.min.js"></script>

    @* This office.js library holds the JavaScript API for Office.
    Replace "beta" in the following line with version number when it is released. *@
    <script src="https://appsforoffice.microsoft.com/lib/beta/hosted/office.js" type="text/javascript"></script>

    @* The following two files hold the Office UI Fabric styles and components. *@
    <link rel="stylesheet" href="https://appsforoffice.microsoft.com/fabric/1.0/fabric.min.css">
    <link rel="stylesheet" href="https://appsforoffice.microsoft.com/fabric/1.0/fabric.components.min.css">
</head>
<body class="ms-font-m cancelDefaultBrowserIndentation"> 

    <div id="content">
        @RenderBody()
    </div>   
   
    @Scripts.Render("~/bundles/PowerPointAddinMicrosoftGraphASPNETInsertChart")
    @RenderSection("scripts", required: false)
</body>
</html>

